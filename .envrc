goapp=`which goapp 2>&1`
if [[ ! "${?}" = "0" ]]; then
  echo "Need to install `goapp` to execute this script."
  exit 127
fi
GOAPPROOT="`dirname $goapp`/goroot"
 
export GOROOT="${GOAPPROOT}"
export GOPATH="`pwd`"          # Should we set external path? http://qiita.com/sinmetal/items/71cfba4ae27cc2366572
export GOBIN="${GOPATH}/bin"
# export PATH=${GOBIN}:${PATH} # If needed
